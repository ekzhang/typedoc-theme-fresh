{
  "name": "typedoc-theme-fresh",
  "version": "0.1.0",
  "description": "A clean, minimalist and well-designed theme for TypeDoc",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "typedoc",
    "theme",
    "documentation"
  ],
  "author": "Eric Zhang <ekzhang1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "node scripts/dev-server.js",
    "example": "typedoc --plugin ./dist/index.js --theme fresh --out docs-example --tsconfig example/tsconfig.json example/src/index.ts",
    "clean": "rm -rf dist docs-example"
  },
  "peerDependencies": {
    "typedoc": "^0.28.14"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.8.1",
    "chokidar": "^4.0.0",
    "serve": "^14.2.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.6.0"
  }
}
